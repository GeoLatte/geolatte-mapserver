        <TileMaps>


            <TileMap
                    title="orthos"
                    type="file"
                    TileImageSourceFactory="org.geolatte.mapserver.tms.FileTileImageSourceFactory"
                    path="src/test/resources/tiles/tilemapresource.xml"
                    srs="25831 9100913 4326"
                    />

        </TileMaps>
    </Producers>
</config>
